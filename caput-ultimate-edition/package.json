  "private": true,
  "type": "module",
  "bin": {
    "genesis": "./src/cli/genesis.js",
    "cert-master": "./src/cli/cert-master.js",
    "genesis-evidence": "./src/cli/evidence.js",
    "genesis-db": "./src/cli/db.js",
    "genesis-ui": "./src/ui/dashboard-server.js",
    "genesis-mabul": "./src/cli/mabul.js",
    "mabul": "./src/cli/mabul.js",
    "genesis-boot": "./src/lib/genesis-init.js",
    "genesis-control": "./src/cli/genesis-control.js"
  },
  "scripts": {
    "genesis": "node src/cli/genesis.js",
    "cert": "node src/cli/cert-master.js",
    "connect": "node src/cli/connect.js",
    "health": "node src/cli/genesis.js health",
    "pentagon": "node src/cli/genesis.js pentagon",
    "network": "node src/cli/genesis.js network",
    "ai": "node src/cli/genesis.js ai",
    "evidence": "node src/cli/evidence.js",
    "evidence:interactive": "node src/cli/evidence.js interactive",
    "evidence:list": "node src/cli/evidence.js list",
    "evidence:verify": "node src/cli/evidence.js verify",
    "evidence:export": "node src/cli/evidence.js export",
    "db": "node src/cli/db.js",
    "db:init": "node src/cli/db.js init",
    "db:health": "node src/cli/db.js health",
    "db:ledger": "node src/cli/db.js ledger",
    "db:search": "node src/cli/db.js search",
    "daemon": "node -e \"import('./src/lib/daemon.js').then(m => m.runDaemon())\"",
    "setup": "node scripts/setup.js",
    "start": "npx tsx src/server.ts",
    "dev": "npx tsx watch src/server.ts",
    "gate": "npx tsx src/index.ts",
    "build": "tsc",
    "test": "node --import tsx --test test/*.test.ts",
    "harden": "npx tsx src/index.ts harden",
    "ooda": "npx tsx src/index.ts ooda",
    "verify": "./verify/full_verify.sh --verbose",
    "orchestrate": "python3 core/orchestrator.py",
    "master": "./bin/genesis_master.sh",
    "rust:build": "cargo build --release --manifest-path rust_boundary/Cargo.toml",
    "rust:test": "cargo test --manifest-path rust_boundary/Cargo.toml",
    "ui": "node src/ui/dashboard-server.js",
    "ui:dev": "node --watch src/ui/dashboard-server.js",
    "dashboard": "node src/ui/dashboard-server.js",
    "mabul": "node src/cli/mabul.js",
    "mabul:status": "node src/cli/mabul.js status",
    "mabul:health": "node src/cli/mabul.js health",
    "mabul:list": "node src/cli/mabul.js list",
    "mabul:search": "node src/cli/mabul.js search",
    "mabul:checkpoint": "node src/cli/mabul.js checkpoint",
    "control": "node src/cli/genesis-control.js",
    "control:status": "node src/cli/genesis-control.js status",
    "control:health": "node src/cli/genesis-control.js health",
    "control:modules": "node src/cli/genesis-control.js modules",
    "boot": "node src/lib/genesis-init.js",
    "boot:sentinel": "node src/lib/genesis-init.js --watch-level active",
    "test:modules": "node --test test/genesis-modules.test.js",
    "test:all": "node --import tsx --test test/*.test.ts && node --test test/*.test.js"
  },
  "dependencies": {
    "tsx": "^4.7.0"
  },
  "optionalDependencies": {
    "pg": "^8.11.0",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
